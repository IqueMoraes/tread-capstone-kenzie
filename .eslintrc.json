{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "import/prefer-default-export": "off",
    "no-unused-vars": "off",
    "indent":"off",
    "import/no-unresolved": "off",
    "import/extensions":"off",
    "max-len":"off",
    "no-return-await":"off",
    "arrow-body-style":"off",
	"no-console": "off"
  }
}
